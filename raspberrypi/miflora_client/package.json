{
  "name": "MifloraServer",
  "version": "",
  "dependencies": {
    "@achingbrain/ssdp": "^2.1.2",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "ip": "^1.1.5",
    "lowdb": "^1.0.0",
    "macaddress": "^0.2.8",
    "noble": "git+https://github.com/PolideaInternal/noble.git#macos_highsierra"
  },
  "scripts": {
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "watch": "nodemon --exec npm start",
